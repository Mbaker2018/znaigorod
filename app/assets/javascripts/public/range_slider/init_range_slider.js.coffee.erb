@init_range_slider = () ->
  range_slider_block = $('.range_slider')

  range_slider_block.each ->
    $this = $(this)
    max = $this.attr('data-count')
    $this.slider({
      range: true
      min: 0
      max: max
      values: [ 1, 3 ]
      slide: (event, ui) ->
        context = $(this).attr('id')
        min = ui.values[0]
        max = ui.values[1] - 1
        min_value =  $('#'+context).parent().find('#item_'+min).attr('data-value')
        max_value =  $('#'+context).parent().find('#item_'+max).attr('data-value')
        console.log min_value
        console.log max_value

      create: (event, ui) ->
        $('.ui-slider-handle').filter(":even").addClass('left_handle')
        $('.ui-slider-handle').filter(":odd").addClass('right_handle')
    })
