<% review.gallery_images.each do |image| %>
  <li>
    <%= link_to image.file_url, :class => :colorbox, :rel => 'gallery' do %>
      <%= image_tag resized_image_url(image.file_url, 143, 109), :size => '143x109', :alt => image.description %>
      <%= link_to "Удалить", my_review_gallery_image_path(review, image), :method => :delete, :remote => true, :class => 'ajax_delete' %>
    <% end %>
  </li>
<% end %>
