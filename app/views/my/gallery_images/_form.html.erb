<div class="upload_wrapper">
  <script id="template-upload" type="text/x-tmpl">
    <div class="upload">
      {%=o.name%}
    <div class="progress"><div class="bar" style="width: 0%"></div></div>
    </div>
  </script>
</div>

<ul class="gallery_images">
</ul>

<%= simple_form_for current_user.account.gallery_images.new, :url => my_gallery_images_path, :html => { :multipart => true, :class => 'file_upload' } do |f| %>
  <%= f.input :file, :input_html => { :multiple => true, :name => 'gallery_images[file]' }, :label => t('my.add_files') %>
  <%= hidden_field_tag :account_id, current_user.account_id  %>
<% end %>
