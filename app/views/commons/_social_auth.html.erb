  </li>
  <li class='yandex'>
    <%= link_to 'Яндекс', user_omniauth_authorize_path(:yandex, :subdomain => request.subdomain), :title => 'Войти с помощью Яндекс' %>
  </li>
  <li class='facebook'>
    <%= link_to 'Facebook', user_omniauth_authorize_path(:facebook, :subdomain => request.subdomain), :title => 'Войти с помощью Facebook' %>
