<% if @placed_banners_first.present? %>

  <div class="placed_banner_wrapper placed_banner11">
    <ul class='banner11'>

      <% @placed_banners_first.each do |banner| %>
        <li class='js-banner'>
          <%= link_to (image_tag banner.image_url, size: "1200x100"), banner.url, :target => banner.target %>
        </li>
      <% end %>

    </ul>
  </div>

<% end %>
