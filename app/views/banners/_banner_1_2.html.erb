      <%= link_to (image_tag 'public/banners/banner_ars.jpg', :size => '880x82', :alt => 'Кастинг: Детская супермодель'), '/away?to=http://ars-alliance.ru/projects/dsm', target: '_blank', class: 'js-stat-dsm' %>
    </li>
  <% end %>

  <% if Time.zone.now < Time.zone.parse('23-02-2015 00:00') %>
    <li class='js-banner wedding'>
      <%= link_to (image_tag 'public/banners/banner_wedding.jpg', :size => '880x82', :alt => 'Кастинг: Детская супермодель'), '/away?to=http://www.weddingshowtomsk.ru/', target: '_blank', class: 'js-stat-wedding' %>
