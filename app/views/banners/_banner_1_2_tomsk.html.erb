<div class="banner12_contest_wrapper <%= params['controller'].to_s.split('/').try(&:first) %>">
  <ul class='banner12 <%= params['controller'].to_s.split('/').try(&:first) %>'>
    <li class='js-banner miss-tomsk-2015-banner'>
      <%= link_to (image_tag 'public/banners/miss-tomsk.jpg', size: '880x80'), contest_path('miss-tomsk-2015'), class: 'js-stat-miss-tomsk' %>
    </li>

    <% unless params[:id] == 'tvoy-shashlyk-samyy-vkusnyy' %>
      <li class='js-banner shashlyk-banner'>
        <%= link_to (image_tag 'public/banners/banner_shashlyk.png', size: '880x80'), contest_path('tvoy-shashlyk-samyy-vkusnyy'), class: 'js-stat-shashlyk' %>
      </li>
    <% end %>
  </ul>
</div>
