<div class="banner12_contest_wrapper <%= params['controller'].to_s.split('/').try(&:first) %>">
  <ul class='banner12 <%= params['controller'].to_s.split('/').try(&:first) %>'>
    <% unless params[:id] == 'proyti-kvest-put-tomicha' %>
      <li class='js-banner quest-banner'>
        <%= link_to (image_tag 'public/banners/banner_1_2_put_tomicha_banner.jpg', size: '880x80'), contest_path('proyti-kvest-put-tomicha'), class: 'js-stat-quest' %>
      </li>
    <% end %>

    <% unless params[:id] == 'miss-pivovarochka-2015-v-tomske' %>
      <li class='js-banner pivovarochka-banner'>
        <%= link_to (image_tag 'public/banners/banner_1_2_miss_pivovarochka.jpg', size: '880x80'), contest_path('miss-pivovarochka-2015-v-tomske'), class: 'js-stat-pivovarochka' %>
      </li>
    <% end %>

    <% unless params[:id] == 'kubok-sodruzhestva-po-karaoke' %>
      <li class='js-banner kubok-banner'>
        <%= link_to (image_tag 'public/banners/banner_1_2_turni_karaoke.jpg', size: '880x80'), contest_path('kubok-sodruzhestva-po-karaoke'), class: 'js-stat-turnir-karaoke' %>
      </li>
    <% end %>
  </ul>
</div>
