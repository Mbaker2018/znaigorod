<% if feed_vote.feedable.voteable.is_a?(Discount) %>
  <%= render partial: "feeds/feed_vote_discount", :object => feed_vote %>
<% else %>
  <%= render partial: "feeds/feed_vote_#{feed_vote.feedable.voteable.class.name.underscore}", :object => feed_vote unless feed_vote.feedable.voteable.nil? %>
<% end %>
