<% decorator = OrganizationDecorator.new feed_visit_organization.visitable %>


<div class='poster'>
<% unless feed_visit_organization.visitable.logotype_url == "" %>
    <%= link_to image_tag(resized_image_url(feed_visit_organization.visitable.logotype_url, 65 , 65 ), :size => '65x65'), feed_visit_organization.visitable %>
<% else %>
  <%= link_to content_tag( :span, "",:class => "stub_poster " + feed_visit_organization.visitable.priority_suborganization_kind.first) %>
<% end %>
</div>
<div class='main'>

  <% if @account.id == feed_visit_organization.user.account.id %>
    <div class='type'>
        <%= t "account.i_want_to_visit.#{feed_visit_organization.user.account.gender}" %>
    </div>
  <% else %>
    <div class='avatar'>
      <%= link_to image_tag( resized_image_url(feed_visit_organization.user.account.avatar_url, 23 , 23 ), :size => '23x23', :title => feed_visit_organization.user.name), feed_visit_organization.user.account  %>
    </div>
    <div class='type'>
      <%= link_to feed_visit_organization.user.name, feed_visit_organization.user.account  %>
        <%= t "account.wants_to_visit_downcase.#{feed_visit_organization.user.account.gender}" %>
    </div>
  <% end %>

  <div class='title'>
<%= link_to(feed_visit_organization.visitable.title, feed_visit_organization.visitable) %>
        <div class='place'>
          <%= decorator.address_link %>
        </div>
  </div>
  <div class='body'>
    <p>
    </p>
  </div>
</div>
