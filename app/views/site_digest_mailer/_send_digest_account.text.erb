  <% if materials.first.first.is_a?(Account) %>
    <%= t "notice_mailer.account_title"  %>
  <% end %>
<% materials = materials.first if @account.gender == 'male' %>
<% materials = materials.second if @account.gender == 'female' %>

<% materials.each_with_index do |material, index| %>
  <% if (index + 1).odd? %>
    <%= render partial: "site_digest_mailer/account", :locals => { :account => material } %>
  <% else %>
    <%= render partial: "site_digest_mailer/account", :locals => { :account => material } %>
  <% end %>
<% end %>
