<div class="place-header">
  <span class="place-title with-counter">
    %title%
  </span>
  <span class="place-counter">
    <%= link_to '%item_url%' do %>
      все <%= presenter.total_count %>
    <% end %>
  </span>
</div>

<div class="place-content accounts">
  <ul>
    <% presenter.collection.each do |item| %>
      <li>
        <%= link_to image_tag(resized_image_url(item.avatar.url, 50, 50), :size => '50x50', :title => item, :alt => item), account_path(item) %>
      </li>
    <% end %>
  </ul>
</div>
