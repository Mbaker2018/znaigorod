<% id = photo.slug.present? ? photo.slug : photo.id.to_s %>
<%= link_to image_tag(resized_image_url(photo.image_url, width, height), :class => 'stub'), photogallery_work_path(@more_photos, photo), :class => id == params[:id] ? 'selected' : 'non-selected' %>
<% if id == params[:id] %>
  <span style='display: none' class='js-start-position'><%= index %></span>
<% end %>
