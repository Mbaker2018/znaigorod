<% if review.gallery_images %>
  <ul class="images">
    <% review.gallery_images.limit(6).each_with_index do |image, index| %>
      <li>
      <% if index == 0 %>
        <%= image_tag resized_image_url(image.file_url, 230, 166), :size => "230x166", :alt => review.title, :title => review.title %>
      <% else %>
        <%= image_tag resized_image_url(image.file_url, 115, 83), :size => "115x83", :alt => review.title, :title => review.title %>
      <% end %>
      </li>
    <% end %>
  </ul>
<% end %>
