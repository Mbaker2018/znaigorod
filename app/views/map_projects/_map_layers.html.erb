<div class='left'>
  <ul>
    <% map_project.map_layers.each do |map_layer| %>
      <%= content_tag :li, :class => map_layer.slug == params[:layer] ? 'selected' : nil do %>
        <%= link_to map_layer.title, february_path(layer: map_layer.slug), :style => "background-image: url('#{map_layer.image_url}')" %>
      <% end %>
    <% end %>
  </ul>

  <div class="contest">
    <%= link_to (image_tag asset_path('public/february_contest/banner.jpg'), size: '250x160'), '/contests/fotokonkurs-par-goroda-tomska-polovinki-loventinki-2' %>
  </div>

  <div class='actual_reviews'>
    <a href='#actual_reviews'>Актуальные обзоры</a>
  </div>

  <div class='text_block'>
    <%= image_tag asset_path('public/february_contest/left.png'), :alt => 'Отдых в новогодние праздники', :class => 'left_wreath' %>
    <p class='important'>Вас нет на карте?</p>
    <p>Звоните нам:</p>
    <p class='phone with_icon'><%= t('services.phone').split(',').first.to_s.squish %></p>
    <p class='phone'><%= t('services.phone').split(',').second.to_s.squish %></p>
    <p>или пишите на</p>
    <p class='email'><%= mail_to 'office@znaigorod.ru' %></p>
  </div>
</div>
